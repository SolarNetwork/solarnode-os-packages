<?xml version="1.0" encoding="UTF-8"?>
<!--
	SolarNode app deployment configuration for Postgres database support.
-->
<ivy-module version="2.0">
	<info organisation="SolarNetwork" module="SolarNode-DB-Postgres"/>
	<configurations>
		<conf name="runtime" visibility="public" description="The Runtime"/>
	</configurations>
	<dependencies defaultconfmapping="runtime->default(runtime)">

		<dependency org="org.postgresql" name="postgresql" rev="42.7.6"/>

		<exclude org="org.checkerframework"/>

	</dependencies>
</ivy-module>
