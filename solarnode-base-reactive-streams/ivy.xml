<?xml version="1.0" encoding="UTF-8"?>
<!--
	SolarNode base platform support for Reactive Streams.
-->
<ivy-module version="2.0">
	<info organisation="SolarNetwork" module="SolarNode-Spring-Security"/>
	<configurations>
		<conf name="runtime" visibility="public" description="The Runtime"/>
	</configurations>
	<dependencies defaultconfmapping="runtime->default(runtime)">

		<dependency org="org.reactivestreams" name="reactive-streams" rev="1.0.4"/>
		<dependency org="io.projectreactor" name="reactor-core" rev="3.5.3"/>

	</dependencies>
</ivy-module>
