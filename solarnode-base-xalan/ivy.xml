<?xml version="1.0" encoding="UTF-8"?>
<!--
	SolarNode app deployment configuration for Xalan/Xerces XML support.
-->
<ivy-module version="2.0">
	<info organisation="SolarNetwork" module="SolarNode-Jackson"/>
	<configurations>
		<conf name="runtime" visibility="public" description="The Runtime"/>
	</configurations>
	<dependencies defaultconfmapping="runtime->default(runtime)">

		<dependency org="org.apache.servicemix.bundles" name="org.apache.servicemix.bundles.bcel" rev="5.2_4"/>
		<dependency org="org.apache.servicemix.bundles" name="org.apache.servicemix.bundles.xalan" rev="2.7.3_3"/>
		<dependency org="org.apache.servicemix.bundles" name="org.apache.servicemix.bundles.xalan-serializer" rev="2.7.3_1"/>
		<dependency org="org.apache.servicemix.bundles" name="org.apache.servicemix.bundles.xerces" rev="2.12.2_1"/>
		<dependency org="org.apache.servicemix.bundles" name="org.apache.servicemix.bundles.xmlresolver" rev="1.2_5"/>

	</dependencies>
</ivy-module>
