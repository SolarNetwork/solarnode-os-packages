<?xml version="1.0" encoding="UTF-8"?>
<!--
	SolarNode app deployment configuration for javax legacy support.
-->
<ivy-module version="2.0">
	<info organisation="SolarNetwork" module="SolarNode-Javax-Legacy"/>
	<configurations>
		<conf name="runtime" visibility="public" description="The Runtime"/>
	</configurations>
	<dependencies defaultconfmapping="runtime->default(runtime)">

		<dependency org="jakarta.activation" name="jakarta.activation-api" rev="1.2.1"/>
		<dependency org="com.sun.activation" name="jakarta.activation" rev="1.2.1"/>
		<dependency org="jakarta.annotation" name="jakarta.annotation-api" rev="1.3.5"/>
		<dependency org="jakarta.validation" name="jakarta.validation-api" rev="2.0.1"/>
		<dependency org="org.apache.servicemix.bundles" name="org.apache.servicemix.bundles.javax-inject" rev="1_3"/>

	</dependencies>
</ivy-module>
